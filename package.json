{
  "author": "kohded",
  "dependencies": {
    "@nestjs/common": "^8.0.4",
    "@nestjs/core": "^8.0.4",
    "@nestjs/platform-express": "^8.0.4",
    "immer": "^9.0.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "use-immer": "^0.6.0"
  },
  "description": "Nest starter application.",
  "devDependencies": {
    "@nestjs/cli": "^8.0.2",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.4",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.1",
    "jest": "27.0.6",
    "lint-staged": "^11.1.1",
    "mkdirp": "^1.0.4",
    "prettier": "^2.3.2",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "license": "MIT",
  "name": "nest-starter",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier \"**/*.{js,json,md,ts,yml}\" -c -w",
    "lint": "eslint \"**/*.{js,ts}\"",
    "prebuild": "rimraf build && npm run format && npm run lint && npm run test",
    "setup:husky": "husky install && husky add \".husky/pre-commit\" \"npx --no-install lint-staged\"",
    "setup:ssl": "mkdirp \"config/ssl\" && mkcert -cert-file \"config/ssl/cert.pem\" -key-file \"config/ssl/key.pem\" localhost",
    "start": "rimraf build && nest start",
    "start:debug": "rimraf build && nest start --debug --watch",
    "start:dev": "rimraf build && nest start --watch",
    "start:prod": "node build/main",
    "test": "jest --ci",
    "test:coverage": "jest --ci --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --no-cache --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "version": "0.1.0"
}
